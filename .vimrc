set autoindent
set backspace=indent,eol,start
set clipboard+=unnamed
set colorcolumn=80
set confirm
set cursorline
set expandtab
set hidden
set hlsearch
set ignorecase
set incsearch
set list
set number
set ruler
set shiftwidth=4
set showcmd
set showmatch
set showmode
set smartcase
set tabstop=4
set title
set virtualedit=onemore
set wildmode=list:longest
set wrapscan
set whichwrap=h,l

syntax on
syntax enable

inoremap <C-e> <esc>
vnoremap <C-e> <esc>

